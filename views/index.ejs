<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/styles.css">
  <title>Superheroes</title>
</head>
<body>

  <section class="main__heroes">

    <h1>Heroes APP</h1>

    <% heroes.forEach(element => { %>

      <article>
        <a href="/detail/<%= element.id %> ">
          <h3><%= element.superhero  %> </h3>
          <img src="/img/<%= element.id %>.jpg" alt="">
        </a>
        <ul>
          <li> <%= element.publisher %> </li>
          <li> <%= element.alter_ego %>  </li>
          <li> <%= element.first_appearance %>  </li>
          <li> <%= element.characters %>  </li>
        </ul>

      </article>
     
    <% }) %>

    

    
  </section>
</body>
</html>